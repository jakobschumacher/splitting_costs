{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", { "code": 100 }],
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}